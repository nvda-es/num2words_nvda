msgstr "Ninguno"

#: addon/globalPlugins/num2word.py:132
msgid "cardinal"
msgstr "cardenal"

#: addon/globalPlugins/num2word.py:132
msgid "Ordinal"
msgstr "Ordinal"

#: addon/globalPlugins/num2word.py:132
